<!-- Floating Action Button -->
<div class="fabs">
    <a href="#top" class="fab toTop">
        <i class="material-icons">expand_less</i>
    </a>
    
    <a href="#bottom" class="fab toBottom">
        <i class="material-icons">keyboard_arrow_down</i>
    </a>
    
    <a id="prime" class="fab">
        <i class="material-icons prime-i-add">add</i>
    </a>
</div>

<!--Footer-->
<footer class="mdl-mini-footer" id="bottom">
    <!--Footer Left Section-->
    <div class="mdl-mini-footer--left-section">
        <div class="mdl-mini-footer--left-section">
            
            <% if(theme.sns.twitter == undefined){ %>
            <% } else{ %>
            <a href="<%= theme.sns.twitter %>" target="view_window"><button class="mdl-mini-footer--social-btn social-btn social-btn__twitter" style="background-image: url(<%= theme.img.footerico %>twitter.png);">
                <span class="visuallyhidden">Twitter</span>
            </button></a>
            <% } %>
            
            <% if(theme.sns.facebook == undefined){ %>
            <% }else{ %>
            <a href="<%= theme.sns.facebook %>" target="view_window"><button class="mdl-mini-footer--social-btn social-btn social-btn__facebook" style="background-image: url(<%= theme.img.footerico %>facebook.png);">
                <span class="visuallyhidden">Facebook</span>
            </button></a>
            <% } %>

            <% if(theme.sns.googleplus == undefined){ %>
            <% }else{ %>
            <a href="<%= theme.sns.googleplus %>" target="view_window"><button class="mdl-mini-footer--social-btn social-btn social-btn__gplus" style="background-image: url(<%= theme.img.footerico %>gplus.png);">
                <span class="visuallyhidden">Google Plus</span>
            </button></a>
            <% } %>
            
            <% if(theme.sns.weibo == undefined){ %>
            <% }else{ %>
            <a href="<%= theme.sns.weibo %>" target="view_window"><button class="mdl-mini-footer--social-btn social-btn social-btn__gplus" style="background-image: url(<%= theme.img.footerico %>gplus.png);">
                <span class="visuallyhidden">Google Plus</span>
            </button></a>
            <% } %>
        </div>
    </div>

